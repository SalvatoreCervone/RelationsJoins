{
    "name": "salvatorecervone/relationsjoins",
    "description": "This is my package relationsjoins",
    "keywords": [
        "SalvatoreCervone",
        "relationsjoins"
    ],
    "homepage": "https://github.com/salvatorecervone/relationsjoins",
    "license": "MIT",
    "authors": [
        {
            "name": "'salvatore'",
            "email": "'salvatore999@virgilio.it'",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "SalvatoreCervone\\RelationsJoins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SalvatoreCervone\\RelationsJoins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
